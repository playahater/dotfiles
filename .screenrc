escape ``
setenv LC_CTYPE en_US.UTF-8
defutf8 on
setenv DISPLAY ':0'
#term screen-256color-bce
term xterm-256color
termcapinfo xterm* ti@:te@
attrcolor b ".I"
#defmonitor on
shell zsh
defscrollback 1000
select 0
altscreen on
nethack on
nonblock on
defnonblock 1
startup_message off
vbell off
autodetach on
deflogin on
defflow off
msgwait 1
acladd play
hardstatus alwayslastline
sorendition +b kG
hardstatus string '%{= kG}[ %=%{W}%?%-Lw%?%{g}(%{W}%n*%f %t%?(%u)%?%{g})%{W}%?%+Lw%?%?%= %{g}][%{B} %{W}%l %{g}]'
#hardstatus alwayslastline "%{=b}%{G} : %{b}%w %=%{kG}%C%A"

screen    -t root                  0
screen    -t alpha                  1

#screen 2 ssh agena7@192.168.0.114
