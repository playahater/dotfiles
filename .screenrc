escape ``
setenv LC_CTYPE en_US.UTF-8
defutf8 on
setenv DISPLAY ':0'
term xterm-256color
termcapinfo xterm* ti@:te@
shell zsh
shelltitle ''
defscrollback 1000
select 0
altscreen on
nethack on
nonblock on
defnonblock 1
startup_message off
vbell off
autodetach on
deflogin on
defflow off
msgwait 1
acladd play
hardstatus alwayslastline
sorendition +b kG
hardstatus string "%{.kW}%-w%{+b r}%n %t%{-}%+w"
