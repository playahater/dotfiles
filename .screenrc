escape ``
attrcolor b ".I"
setenv LC_CTYPE en_US.UTF-8
encoding utf8 utf8
defencoding utf8
defutf8 on
utf8 on
defutf8 on
setenv DISPLAY ':0'
term xterm-256color
#termcapinfo xterm* ti@:te@
termcapinfo xterm*|rxvt*|urxvt* 'hs:ts=\E]2;:fs=\007:ds=\E]2;screen\007'
hardcopy_append on
crlf off
altscreen on
defhstatus "screen: ^En (^Et)"
shell zsh
shelltitle ''
defmonitor on
defbce on
defscrollback 3000
select 0
nethack on
nonblock on
defnonblock 1
startup_message off
vbell off
autodetach on
deflogin on
defflow off
msgwait 1
acladd play
sorendition +b kG
hardstatus alwayslastline "%{=b}%{G}%{r}%w"
