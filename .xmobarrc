Config { font = "xft:Literation Mono Powerline:pixelsize=15:antialias=true:autohint=true:hinting=true:dpi=144"
       , borderColor = "black"
       , border = TopB
       , bgColor = "#0B2027"
       , fgColor = "#F6F1D1"
       , position = Top
       , persistent = False
       , hideOnStart = False
       , lowerOnStart = True
       , allDesktops = True
       , overrideRedirect = True
       , pickBroadest = False

       , commands = [ Run Weather "LYBE" ["-t","<tempC>","-L","10","-H","28","--normal","#F6F1D1","--high","#E84F4F","--low","#6D878D"] 18000
                    , Run MultiCpu ["-L","0","-H","70","--normal","#F6F1D1","--high","#E84F4F"] 10
                    , Run Memory ["-t","Mem: <used> MB", "-L","7000","-H","14500", "-l", "#F6F1D1", "--normal","#E1AA5D","--high","#E84F4F"] 50
                    , Run Network "wlp3s0" ["-L","0","-H","480","-l", "#F6F1D1", "--normal","#E1AA5D","--high","#E84F4F"] 5
                    , Run Date "%a %b %_d %Y %H:%M" "date" 10
                    , Run Wireless "wlp3s0" [] 10
                    , Run TopProc ["-l", "#F6F1D1", "--normal","#E1AA5D","--high","#E84F4F"] 10
                    , Run ThermalZone 0 ["-t","<temp> C", "-L","0","-H","70","--normal","#F6F1D1","--high","#E84F4F"] 30
                    , Run BatteryN ["BAT0"] ["-t", "<acstatus><watts> (<left>%)", "-L", "10", "-H", "80", "-p", "3", "--", "-O", "<fc=#F6F1D1>on</fc> - ", "-o", "", "-L", "-15", "-H", "-5", "-l", "#E84F4F", "-m", "blue", "-h", "green"] 600 "bat0"
                    , Run BatteryN ["BAT1"] ["-t", "<acstatus><watts> (<left>%)", "-L", "10", "-H", "80", "-p", "3", "--", "-O", "<fc=#F6F1D1>on</fc> - ", "-o", "", "-L", "-15", "-H", "-5", "-l", "#E84F4F", "-m", "blue", "-h", "green"] 600 "bat1"
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{<fc=#F6F1D1>|</fc> %LYBE% <fc=#F6F1D1>|</fc> %multicpu% <fc=#F6F1D1>/</fc> %top% <fc=#F6F1D1>/</fc> %thermal0% <fc=#F6F1D1>|</fc> %memory% <fc=#F6F1D1>|</fc> %wlp3s0% <fc=#F6F1D1>/</fc> %wlp3s0wi% <fc=#F6F1D1>|</fc> Bat0: %bat0% <fc=#F6F1D1>/</fc> Bat1: %bat1% <fc=#F6F1D1>|</fc><fc=#F6F1D1> %date% </fc>"
       }
