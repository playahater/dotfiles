{
"parser": "typescript-eslint-parser",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true,
    "mongo": true,
    "webextensions": true
  },
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          experimentalObjectRestSpread: true
      }
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "extends": [
      "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-reserved-keys": 0,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "eqeqeq": 0,
    "radix": 0,
    "no-shadow-restricted-names": 2,
    "camelcase": 0,
    "comma-spacing": 2,
    "comma-style": 2,
    "consistent-this": 0,
    "func-names": 0,
    "func-style": 0,
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true
    }],
    "max-nested-callbacks": 0,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-inline-comments": 0,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }],
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-underscore-dangle": 0,
    "one-var": 0,
    "operator-assignment": [2, "always"],
    "padded-blocks": 0,
    "quote-props": [2, "as-needed"],
    "sort-vars": [2, {"ignoreCase": true}],
    "array-bracket-spacing": [2, "never"],
    "space-in-parens": 2,
    "space-unary-ops": 2,
    "wrap-regex": 0,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "jsx-quotes": [2, "prefer-double"],
    "react/display-name": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-did-mount-set-state": 1,
    "jsx-a11y/alt-text": 0,
    "jsx-a11y/href-no-hash": 0
  }
}
