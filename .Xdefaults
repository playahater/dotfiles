Xft.antialias: true
Xft.dpi:       96
Xft.hinting:   true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba:      rgb

URxvt.depth:                32
URxvt.termName:             rxvt-unicode
URxvt.loginShell:           true
URxvt.letterSpace:          -1
URxvt.utmpInhibit:          true
URxvt.scrollBar:            false
URxvt.internalBorder:       1
URxvt.borderWidth:          0
URxvt.secondaryScroll:      false
URxvt.saveLines:            128
URxvt.cursorBlink:          false
URxvt.buffered:             false
URxvt.urgentOnBell:         true
URxvt.foreground:           White
URxvt.cursorColor:          Gray
URxvt.background:           Black
URxvt.perl-lib:             /usr/lib/urxvt/perl
!URxvt.matcher.button:      1
URxvt.perl-ext-common:      searchable-scrollback,default,clipboard
URxvt.font:                 xft:Liberation Mono:pixelsize=14:antialias=true:autohint=true:dpi=96
URxvt.boldFont:             xft:Liberation Mono:pixelsize=14:antialias=true:autohint=true:dpi=96
URxvt*transparent:          false
URxvt*shading:              0
URxvt*tint:                 Black
URxvt*inheritPixmap:        true
