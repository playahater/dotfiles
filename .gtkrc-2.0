gtk-xft-hintstyle="hintfull"
gtk-xft-rgba="rgb"
gtk-color-scheme="sidebar_background:#313233\ntheme_text_dark_color:#dddddd\ntooltip_fg_color:#ffffff\nlink_color:#4a90f9\nbase_color:#2a2a2a\ntheme_bg_color:#383838\nbutton_base:#4c4d4e\ntext_color:#dddddd\nselected_fg_color:#ebebeb\ntheme_bg_dark_color:#383838\nbg_color:#383838\ntooltip_bg_color:#303030\ntheme_fg_dark_color:#c0c0c0\nselected_bg_color:#496490\nfg_color:#c0c0c0\n"
include "/home/play/.gtkrc-2.0.mine"
